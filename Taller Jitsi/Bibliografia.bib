  author = {{Vidal-Silva}, Cristian L. and {S{\'a}nchez-Ortiz}, Aurora and Serrano, Jorge and Rubio, Jos{\'e} M. and {Vidal-Silva}, Cristian L. and {S{\'a}nchez-Ortiz}, Aurora and Serrano, Jorge and Rubio, Jos{\'e} M.},
